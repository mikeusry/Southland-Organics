INSERT INTO `cache_content` VALUES ('content:14:14','a:5:{s:23:\"field_product_highlight\";a:1:{i:0;a:2:{s:5:\"value\";s:207:\"Garden Pro will make you the envy of your gardening friends.  Specially formulated for vegetable gardens, Garden Pro Dry is an elemental cocktail that is powerful and long lasting for sustainable production.\";s:6:\"format\";s:1:\"1\";}}s:25:\"field_product_application\";a:1:{i:0;a:2:{s:5:\"value\";s:281:\"Apply a liberal amount of Garden Pro Dry formula to your soil. Recommended rate is 1 pound per every 10 square feet of garden space.\r\n\r\nFor best results apply Garden Pro Dry in the Fall and in the Spring and drench with Jump Start Soil Activator.\r\n\r\nIdeal for container operations.\";s:6:\"format\";s:1:\"1\";}}s:22:\"field_product_benefits\";a:1:{i:0;a:2:{s:5:\"value\";s:364:\"<ul>\r\n<li>Restores fertile soil conditions</li>\r\n<li>Cleans up contaminants in the soil</li>\r\n<li>Retains valuable soluble nutrients</li>\r\n<li>Reduces leaching of fertilizers</li>\r\n<li>Increases nitrogen uptake</li>\r\n<li>Eliminates the need for fungicides through competitive exclusion</li>\r\n<li>Breaks down pesticides and chemicals present in the soil</li>\r\n</ul>\";s:6:\"format\";s:1:\"2\";}}s:17:\"field_promo_image\";a:1:{i:0;N;}s:17:\"field_image_cache\";a:1:{i:0;a:10:{s:3:\"fid\";s:2:\"20\";s:3:\"uid\";s:1:\"1\";s:8:\"filename\";s:19:\"Garden-Pro_logo.png\";s:8:\"filepath\";s:39:\"sites/default/files/Garden-Pro_logo.png\";s:8:\"filemime\";s:9:\"image/png\";s:8:\"filesize\";s:5:\"38263\";s:6:\"status\";s:1:\"1\";s:9:\"timestamp\";s:10:\"1274650403\";s:4:\"list\";s:1:\"1\";s:4:\"data\";a:2:{s:3:\"alt\";s:19:\"Garden Pro Dry Logo\";s:5:\"title\";s:47:\"Garden Pro Dry - Amazing results in vegetables.\";}}}}','0','1305077158','','1'),('content:9:9','a:5:{s:23:\"field_product_highlight\";a:1:{i:0;a:2:{s:5:\"value\";s:504:\"<p>Turf Pro is an <strong>organic lawn treatment</strong> and soil conditioner that is derived from an extremely rare and highly decomposed organic humus deposit. &nbsp;Turf Pro is an unparalleled and superior product to any other soil&nbsp;conditioner. &nbsp;Turf Pro contains a unique bacillus, 72 trace minerals, humic, fulvic and ulmic acids, and other live microbiology.</p>\r\nTry Turf Pro Organic Lawn Treatment today for you lawn and you will see a longer lasting, healthier lawn for years to come.\";s:6:\"format\";s:1:\"2\";}}s:25:\"field_product_application\";a:1:{i:0;a:2:{s:5:\"value\";s:155:\"Apply four ounces of  <strong class=\"weak\">Turf Pro</strong> | <strong class=\"weak\">Organic Lawn Treatment</strong> per 100 square feet on a monthly basis.\";s:6:\"format\";s:1:\"1\";}}s:22:\"field_product_benefits\";a:1:{i:0;a:2:{s:5:\"value\";s:741:\"<ul>\r\n<li>Stimulate the growth of beneficial bacteria and fungi for a healthy root zone (rhizosphere)</li>\r\n<li>Convert nutrients in the soil to more readily available forms</li>\r\n<li>Promote more effective water penetration, transmission, retention and drought tolerance</li>\r\n<li>Provide stored energy for plant growth and resistance to insects and disease</li>\r\n<li>Greatly reduce or eliminate the need for nitrogen fertilization</li>\r\n<li>Serves as a Bio-Stimulant</li>\r\n<li>Includes a full spectrum beneficial biology (bacteria, fungi and enzymes)</li>\r\n<li>Serves as a Soil Conditioner and Nutrient Converter</li>\r\n<li>High content of Humic Acids</li>\r\n<li>High content of Ulmic Acids</li>\r\n<li>High content of Fulvic Acids</li>\r\n</ul>\";s:6:\"format\";s:1:\"1\";}}s:17:\"field_promo_image\";a:1:{i:0;a:10:{s:3:\"fid\";s:2:\"30\";s:3:\"uid\";s:1:\"1\";s:8:\"filename\";s:19:\"Turf_Pro_banner.jpg\";s:8:\"filepath\";s:52:\"sites/default/files/promo_slides/Turf_Pro_banner.jpg\";s:8:\"filemime\";s:10:\"image/jpeg\";s:8:\"filesize\";s:6:\"165988\";s:6:\"status\";s:1:\"1\";s:9:\"timestamp\";s:10:\"1274981854\";s:4:\"list\";s:1:\"1\";s:4:\"data\";a:2:{s:3:\"alt\";s:33:\"Turf Pro - Organic Lawn Treatment\";s:5:\"title\";s:0:\"\";}}}s: